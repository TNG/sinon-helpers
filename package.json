{
  "name": "sinon-helpers",
  "description": "Create easily configurable sinon stubs that mimic constructors and keep track of their instances",
  "author": "Lukas Taegert <lukas.taegert@tngtech.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "jsnext:main": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TNG/sinon-helpers.git"
  },
  "keywords": [
    "sinon",
    "constructor",
    "stub",
    "mock",
    "spy"
  ],
  "bugs": {
    "url": "https://github.com/TNG/sinon-helpers/issues"
  },
  "homepage": "https://github.com/TNG/sinon-helpers#readme",
  "peerDependencies": {
    "sinon": ">=1.15.0"
  },
  "dependencies": {
    "fluent-arguments": "^1.1.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.1",
    "@commitlint/config-angular": "^6.0.2",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-ramda": "^1.6.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-tree-shaking": "^1.7.2",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "lint-staged": "^7.0.0",
    "mocha": "^5.0.0",
    "prettier-standard": "^8.0.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "semantic-release": "^15.0.0",
    "sinon": "^5.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "npm run build && mocha",
    "lint": "prettier-standard *.js src/**/*.js test/**/*.js",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "postcommit": "git reset",
    "prepush": "npm test",
    "postmerge": "npm install && npm prune",
    "postrewrite": "npm install && npm prune",
    "semantic-release": "npm run build && semantic-release pre && npm publish && semantic-release post"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "eslint --fix",
      "git add"
    ]
  },
  "version": "0.0.0-development"
}
